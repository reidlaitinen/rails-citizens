<div class='person-form'>
  <%= form_for @person do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name, :autocomplete => 'off' %>
    <br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name, :autocomplete => 'off' %>
    <br>
    <%= f.label :age %>
    <%= f.text_field :age, :autocomplete => 'off' %>
    <br>
    <%= f.label :hair_color %>
    <%= f.text_field :hair_color, :autocomplete => 'off' %>
    <br>
    <%= f.label :eye_color %>
    <%= f.text_field :eye_color, :autocomplete => 'off' %>
    <br>
    <%= f.label :gender %>
    <%= f.text_field :gender, :autocomplete => 'off' %>
    <br>
    <%= f.label :alive %>
    <%= f.check_box :alive %>
    <%= f.submit %>
  <% end %>
</div>